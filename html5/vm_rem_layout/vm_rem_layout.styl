* 
  -webkit-tap-highlight-color: transparent // 点击高亮
  outline: 0 // 轮廓
  
// 由于*太贪婪了所以有时候为了提高性能
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td
  margin 0
  padding 0
  vertical-align baseline
a, a:visited {
  text-decoration none
  color: #333333
}
img
  border: none 
  vertical-align top
i, em {
  font-style: normal
}
html
  font-size: 20vw
ul li
  list-style: none
body
  max-width 540px
  min-width 320px
  margin 0 auto
  background #e8e8e8
  font-style 14px
  font-family -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif
  line-height 1.5
  color: #666666
  -webkit-text-size-adjust: 100% !important // 禁止调整文字大小
// S轮播样式
.mod_slide
  position relative
.mod_slide_list_item img 
  width 100%
  height auto
  position: absolute
  top: 0
  left 0
.mod_slide_inner
    overflow hidden
.mode_slide_list
  font-size 0
  word-spacing nowrap 
  -webkit-overflow-scrolling: touch
  overflow hidden
  overflow-x: auto
.mode_slide_list_item
  width 100%
  position relative
  height 0
  overflow hidden
  padding-top 45.3333333%
  display inline-block
  vertical-align: top 
